{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch main.mjs",
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "--experimental-modules",
                "${workspaceFolder}/src/main/main.mjs"
            ],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "console": "integratedTerminal"
        }
    ]
}